    <!-- http://www.semanticweb.org/vincenturro/moba#chooseAram -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#chooseAram">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/vincenturro/moba#chooseMatch"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Aram"/>
        <rdfs:comment xml:lang="it">Un Player sceglie una tipologia specifica di match da giocare, un Aram.</rdfs:comment>
        <rdfs:label xml:lang="en">chooseAram</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#chooseChampion -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#chooseChampion">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
        <rdfs:comment xml:lang="it">Un Player sceglie un Champion con cui giocare</rdfs:comment>
        <rdfs:label xml:lang="en">chooseChampion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#chooseClassic -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#chooseClassic">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/vincenturro/moba#chooseMatch"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/vincenturro/moba#chooseMatch"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/vincenturro/moba#Classica"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Classica"/>
        <rdfs:comment xml:lang="it">Un Player sceglie una tipologia specifica di match da giocare, un Classic.</rdfs:comment>
        <rdfs:label xml:lang="en">chooseClassic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#chooseMatch -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#chooseMatch">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Partita"/>
        <rdfs:comment xml:lang="it">Un Player sceglie un match da giocare</rdfs:comment>
        <rdfs:label xml:lang="en">chooseMatch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#composedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#composedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#mappa"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#Core"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#Tower"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#jungle"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#lane"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#spawn"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Una Map è composta da più parti, ogni parte in numero variabile.</rdfs:comment>
        <rdfs:label xml:lang="en">composedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#hasAbility -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#hasAbility">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/vincenturro/moba#isAnAbilityOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Abilità"/>
        <rdfs:comment xml:lang="it">Un Champion ha una o più Ability</rdfs:comment>
        <rdfs:label xml:lang="en">hasAbility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#hasPng -->

