        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Player"/>
        <rdfs:comment xml:lang="it">Un Player può avere uno o più compagni di squadra</rdfs:comment>
        <rdfs:label xml:lang="en">hasTeammate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#isADamageDealer -->

