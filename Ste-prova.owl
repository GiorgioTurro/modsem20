    <!-- http://www.semanticweb.org/vincenturro/moba/hasRoleDamageDealer -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba/hasRoleDamageDealer">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#burst_damage"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#sustain_damage"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#hasRole"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#isADamageDealer"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba/hasRoleFrontLine -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba/hasRoleFrontLine">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#bruiser"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#tank"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#hasRole"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/vincenturro/moba#isAFrontLine"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


