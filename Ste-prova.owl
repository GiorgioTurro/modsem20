    <!-- http://www.semanticweb.org/vincenturro/moba#hasTeam -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#hasTeam">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Minion"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Structure"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Blue</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Red</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
