        <hasAbility rdf:resource="http://www.semanticweb.org/vincenturro/moba#enchanted_crystal_arrow"/>
        <hasAbility rdf:resource="http://www.semanticweb.org/vincenturro/moba#frost_shot"/>
        <hasAbility rdf:resource="http://www.semanticweb.org/vincenturro/moba#hawkshot"/>
        <hasAbility rdf:resource="http://www.semanticweb.org/vincenturro/moba#volley"/>
        <hasAbility rdf:resource="http://www.semanticweb.org/vincenturro/moba#ranger&apos;s_focus"/>
        <isADamageDealer rdf:resource="http://www.semanticweb.org/vincenturro/moba#sustainDamage"/>
        <sameRole rdf:resource="http://www.semanticweb.org/vincenturro/moba#Aphelios"/>
        <attackIs rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">59</attackIs>
        <defenseIs rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">26</defenseIs>
