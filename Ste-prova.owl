        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Oggetto"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
        <rdfs:comment xml:lang="it">Un Champion può usare (Consumare o Equipaggiarsi) di un Item</rdfs:comment>
        <rdfs:label xml:lang="en">isUsedBy</rdfs:label>
    </owl:ObjectProperty>
    


