        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vincenturro/moba#sameRole -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vincenturro/moba#sameRole">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vincenturro/moba#Champion"/>
